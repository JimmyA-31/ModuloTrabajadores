@if (TempData["Success"] != null)
{
    <script>
        Swal.fire({
            icon: 'success',
            title: 'Éxito',
            text: '@TempData["Success"]',
            confirmButtonColor: '#0d6efd'
        });
    </script>
}

@if (TempData["Error"] != null)
{
    <script>
        Swal.fire({
            icon: 'error',
            title: 'Error',
            text: '@TempData["Error"]',
            confirmButtonColor: '#dc3545'
        });
    </script>
}
